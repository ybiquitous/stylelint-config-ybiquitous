{
  "name": "stylelint-config-ybiquitous",
  "version": "0.0.1",
  "description": "My shareable config for stylelint",
  "author": "Masafumi Koba <ybiquitous@gmail.com>",
  "license": "MIT",
  "repository": "ybiquitous/stylelint-config-ybiquitous",
  "main": "index.js",
  "files": [
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "stylelint-config-standard": "^18.2.0"
  },
  "devDependencies": {
    "eslint-config-ybiquitous": "^7.1.0",
    "stylelint": "^9.10.1",
    "tape": "^4.9.2",
    "ybiq": "^6.0.0"
  },
  "scripts": {
    "test": "node test.js",
    "format": "npm-run-all --print-label --parallel lint:js:fix prettier:write",
    "lint": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.jsx,.mjs .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:md": "markdownlint --ignore node_modules --ignore CHANGELOG.md \"**/*.md\"",
    "prettier:write": "prettier --write --ignore-path .gitignore \"**/*.{html,json,md,yaml,yml}\"",
    "release": "standard-version --sign",
    "release:dry-run": "npm run release -- --dry-run"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,mjs}": [
        "eslint --fix --no-ignore",
        "git add"
      ],
      "*.md": [
        "markdownlint --ignore CHANGELOG.md"
      ],
      "*.{html,json,md,yaml,yml}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "CHANGELOG.md"
    ]
  },
  "eslintConfig": {
    "extends": [
      "ybiquitous/node"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  }
}
